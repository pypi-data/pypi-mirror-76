        from elgamal.elgamal import Elgamal<br>
        
        m = b'Text'<br>
        
        print(m)<br>
        
        pb, pv = Elgamal.newkeys(128)<br>
        
        print(pb)<br>
        print(pv)<br>
        
        ct = Elgamal.encrypt(m, pb)<br>
        
        print(ct)<br>
        
        dd = Elgamal.decrypt(ct, pv)<br>
        
        print(dd)<br>
        print()<br>
        
